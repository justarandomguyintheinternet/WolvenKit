<reactiveUi:ReactiveUserControl
    x:Class="WolvenKit.Views.Types.SimpleRedArrayEditorView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:red="clr-namespace:WolvenKit.App.ViewModels.Red;assembly=WolvenKit.App"
    d:DataContext="{d:DesignInstance red:RedArrayViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:TypeArguments="red:RedArrayViewModel"
    mc:Ignorable="d">
    <TextBlock
        Name="PropertyType"
        Foreground="{StaticResource WolvenKitTan}"
        Text="{Binding Type, Mode=OneTime}" />
</reactiveUi:ReactiveUserControl>
